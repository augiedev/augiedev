							<apex:outputLabel styleClass="loginClass" value="{!$Label.site.password}: " for="password"/>
							<apex:inputSecret styleClass="loginClass" id="password" value="{!password}" /><br/>
							<apex:commandButton styleClass="loginClass" action="{!login}" value="{!$Label.site.login_button}" id="loginButton"/><br/>
							<a class="loginClass" href="Register1">Register</a> | 
